# Bubble Sort
def BubbleSort(arr):
    for i in range(1, len(arr)): 
        for j in range(len(arr)-i):
            if arr[j] > arr[j+1]:
                #arr[j], arr[j+1] = arr[j+1], arr[j]
                arr[j] = arr[j] + arr[j+1]
                arr[j+1] = arr[j] - arr[j+1]
                arr[j] -= arr[j+1]
    return arr


if __name__ == '__main__':
    import random
    arr = list(range(20))
    random.shuffle(arr)
    print(arr)
    arr = BubbleSort(arr)
    print(arr)
