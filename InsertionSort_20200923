#Insertion Sort
def InsertionSort(arr):
    i = 1
    while i < len(arr):
        j = i - 1
        temp = arr[i]
        while j >= 0 and arr[j] > temp:
            arr[j+1] = arr[j]
            j-=1
        arr[j + 1] = temp
        print(arr)
        i += 1
    return arr


if __name__ == '__main__':
    import random
    a = list(range(15))
    random.shuffle(a)
    arr = InsertionSort(a)
    print('-'*30)
